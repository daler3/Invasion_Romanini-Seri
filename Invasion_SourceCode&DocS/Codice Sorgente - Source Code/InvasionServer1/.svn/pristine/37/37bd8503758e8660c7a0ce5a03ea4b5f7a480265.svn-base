package classiCondivise;

public enum Colori {
	GIALLO(255,255,0),
	ROSSO(255,0,0),
	VERDE(0,255,0),
	BLU(0,0,255),
	VIOLA(143,0,255),
	NERO(0,0,0);
	
	private Integer r;
	private Integer g;
	private Integer b;
	
	private Colori(Integer r, Integer g, Integer b) {
		this.r = r;
		this.g = g;
		this.b = b;
	}
	
	public Integer getR() {
		return r;
	}
	public Integer getG() {
		return g;
	}
	public Integer getB() {
		return b;
	}
}
